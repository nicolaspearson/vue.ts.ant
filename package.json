{
  "name": "vue.ts.ant",
  "version": "0.0.1",
  "description": "A simple Vue app, written using Typescript and Ant Design",
  "author": "Nicolas Pearson",
  "license": "MIT",
  "scripts": {
    "start:dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "yarn lint:ts",
    "lint:ts": "yarn lint:ts:fix --no-fix",
    "lint:ts:fix": "vue-cli-service lint --fix src test types",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "jest --version && vue-cli-service test:unit --verbose --coverage --config=./tests/unit/jest.config.js --color",
    "test:unit:snapshot": "yarn test:unit --updateSnapshot",
    "ncu": "ncu",
    "ncu:u": "ncu -u"
  },
  "dependencies": {
    "ant-design-vue": "~1.4.11",
    "register-service-worker": "~1.6.2",
    "vue": "~2.6.11",
    "vue-class-component": "~7.2.3",
    "vue-property-decorator": "~8.4.0",
    "vue-router": "~3.1.6",
    "vuex": "~3.1.2"
  },
  "devDependencies": {
    "@types/jest": "~25.1.3",
    "@types/node": "~13.7.7",
    "@vue/cli-plugin-e2e-cypress": "~4.2.3",
    "@vue/cli-plugin-pwa": "~4.2.3",
    "@vue/cli-plugin-typescript": "~4.2.3",
    "@vue/cli-plugin-unit-jest": "~4.2.3",
    "@vue/cli-service": "~4.2.3",
    "@vue/test-utils": "~1.0.0-beta.31",
    "babel-core": "~6.26.3",
    "jest": "~25.1.0",
    "less": "~3.11.1",
    "less-loader": "~5.0.0",
    "node-sass": "~4.13.1",
    "npm-check-updates": "~4.0.2",
    "prettier": "~1.19.1",
    "sass-loader": "~8.0.2",
    "ts-jest": "~25.4.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3",
    "vue-jest": "~3.0.5",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
  }
}
